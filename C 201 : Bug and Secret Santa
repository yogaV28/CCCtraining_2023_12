
code:
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d", &n);

    int p[n], r[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &p[i]);
    }

    for (int i = 0; i < n; i++) {
        r[p[i] - 1] = i + 1;
    }
    for (int i = 0; i < n; i++) {
        printf("%d ", r[i]);
    }

    return 0;
}

